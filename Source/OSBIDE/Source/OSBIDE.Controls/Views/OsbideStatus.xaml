<UserControl x:Class="OSBIDE.Controls.Views.OsbideStatus"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:views="clr-namespace:OSBIDE.Controls.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl>
            <TabItem Header="Downloads">
                <views:AssignmentSubmissions x:Name="SubmissionsTab" ViewModel="{Binding Path=SubmissionViewModel, Mode=TwoWay}" />
            </TabItem>
            <TabItem Header="Status">
                <views:TransmissionStatus x:Name="TransmissionTab" ViewModel="{Binding Path=StatusViewModel, Mode=TwoWay}" />
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
