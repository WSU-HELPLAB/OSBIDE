@model List<OSBIDE.Data.DomainObjects.ErrorQuotientResult>

<table id="error-quotient-result" class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>EQ Score</th>
            <th>Avg Grade</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Model)
        {
            <tr>
                <td>@row.Name</td>
                <td>@row.Score</td>
                <td>@row.Grade</td>
            </tr>
        }
    </tbody>
</table>
